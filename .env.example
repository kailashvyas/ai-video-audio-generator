# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# Optional API Keys (for future use when available)
# MUSICLM_API_KEY=your_musiclm_api_key_here_when_available

# Note: Veo video generation is now available through the Gemini API
# No separate VEO_API_KEY is required - use your GEMINI_API_KEY

# Application Settings
OUTPUT_DIRECTORY=./output
TEMP_DIRECTORY=./temp
MAX_CONCURRENT_REQUESTS=3
DEFAULT_BUDGET_LIMIT=100.00
LOG_LEVEL=info

# Generation Defaults
DEFAULT_MAX_SCENES=5
DEFAULT_QUALITY=standard
DEFAULT_OUTPUT_FORMATS=mp4,webm

# Rate Limiting
REQUESTS_PER_MINUTE=60
BURST_LIMIT=10

# Error Handling
MAX_RETRIES=3
RETRY_DELAY_MS=1000
EXPONENTIAL_BACKOFF=true